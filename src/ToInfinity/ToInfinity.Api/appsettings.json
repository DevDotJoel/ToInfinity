{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "SecurityKey": "REPLACE_WITH_SECURE_KEY_256_BITS_MINIMUM",
    "Issuer": "https://api-toinfinity.com",
    "Audience": "https://toinfinity.com",
    "ExpiryMinutes": 60
  },
  "WebApp": {
    "FrontendPublicUrl": "https://toinfinity.com",
    "BackendPublicUrl": "https://api-toinfinity.com"
  },
  "Google": {
    "ClientId": "REPLACE_WITH_PRODUCTION_GOOGLE_CLIENT_ID",
    "ClientSecret": "REPLACE_WITH_PRODUCTION_GOOGLE_CLIENT_SECRET"
  },
  "Stripe": {
    "SecretKey": "REPLACE_WITH_STRIPE_SECRET_KEY",
    "PublishableKey": "REPLACE_WITH_STRIPE_PUBLISHABLE_KEY",
    "WebhookSecret": "REPLACE_WITH_STRIPE_WEBHOOK_SECRET",
    "SuccessUrl": "https://toinfinity.com/subscription/success?session_id={CHECKOUT_SESSION_ID}",
    "CancelUrl": "https://toinfinity.com/subscription/canceled",
    "Plans": {
      "Basic": {
        "PriceId": "price_xxxxxxxxxxxxx",
        "Amount": 650.00,
        "Name": "Basic Plan"
      },
      "Professional": {
        "PriceId": "price_xxxxxxxxxxxxx",
        "Amount": 1400.00,
        "Name": "Professional Plan"
      },
      "Elite": {
        "PriceId": "price_xxxxxxxxxxxxx",
        "Amount": 2200.00,
        "Name": "Elite Plan"
      }
    }
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "POST:/api/auth/login",
        "Period": "1m",
        "Limit": 5
      },
      {
        "Endpoint": "POST:/api/auth/register",
        "Period": "5m",
        "Limit": 3
      },
      {
        "Endpoint": "POST:/api/auth/refresh",
        "Period": "1m",
        "Limit": 10
      },
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 100
      }
    ]
  }
}
